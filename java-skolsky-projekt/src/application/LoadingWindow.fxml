<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="150.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoadingWindowControler">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Select COM port:">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </Text>
            <FlowPane alignment="CENTER" columnHalignment="CENTER" prefHeight="11.0" prefWidth="200.0">
               <children>
                  <ComboBox fx:id="ListOfCom" prefHeight="25.0" prefWidth="107.0">
                     <FlowPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </FlowPane.margin>
                  </ComboBox>
                  <Button mnemonicParsing="false" onAction="#ButtonRefreshClicked" text="Refresh">
                     <FlowPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </FlowPane.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </FlowPane>
            <Button mnemonicParsing="false" onAction="#ButtonConnectClicked" text="Connect">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
